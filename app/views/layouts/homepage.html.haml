%head
  = render 'layouts/core/head', responsive: true, homepage: true, suppress_tynt: (defined?(suppress_tynt) ? suppress_tynt : false)

%body.responsive.homepage
  .wrapper.js-wrapper
    = render 'layouts/core/snippets/accessibility'
    = render 'layouts/core/snippets/primary_navigation_bar', user_nav: true, responsive: true, homepage: true, no_scripts: false

    .row.row--content#js-row--content
      = yield

    .wrapper.wrapper--footer
      = render 'layouts/core/snippets/body_footer', responsive: true

    = render 'layouts/core/snippets/pointer_cover'
    = render 'layouts/core/snippets/footer_js'
    = render 'layouts/core/inline_js/img_loader'

    = render 'layouts/core/snippets/one_by_one'

    - if defined? Variant
      /
        = "build: #{Variant.full_namespace}"
